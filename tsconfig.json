{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl":".",
    "paths": {
      "@componets/*":["src/componets/*"],
      "@*": ["src/*"],
      "@assets/*":["assets/*"],
      "@constants/*":["src/constants/*"],
      

    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
